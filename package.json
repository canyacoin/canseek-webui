{
  "name": "canseek-webui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build --prod --build-optimizer",
    "build:dev": "ng build --configuration=dev --build-optimizer --output-path=dist-dev/canseek",
    "zip:prod": "zip dist-prod.zip dist-prod/*",
    "zip:dev": "zip dist-dev.zip dist-dev/*",
    "upload:prod": "scp -i ../aliyun2.pem dist-prod.zip root@47.94.194.126:/root",
    "upload:dev": "scp -i ../aliyun2.pem dist-dev.zip root@47.94.194.126:/root/CanSeek",
    "deploy:prod": "npm run build:prod && npm run zip:prod && npm run upload:prod",
    "deploy:dev": "npm run build:dev && npm run zip:dev && npm run upload:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@canyaio/canpay-lib": "0.1.20",
    "angularfire2": "5.0.0-rc.9",
    "core-js": "^2.5.4",
    "firebase": "5.0.2",
    "gravatar": "^1.6.0",
    "less": "^2.7.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ng-zorro-antd": "^1.4.0",
    "ngx-clipboard": "^11.1.1",
    "rxjs": "^6.0.0",
    "web3": "^1.0.0-beta.35",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "git+https://github.com/Xinjie-canya/angular-devkit-build-angular-builds.git#v1.0.3",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "protractor": "~5.3.0",
    "rxjs-compat": "^6.2.0",
    "truffle-contract": "^3.0.6",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
